{
  "esmcat_version": "0.1.0",
  "id": "gfdltest",
  "description": "This is the test data collection in S3 public bucket comprising netCDF data from CMIP6",
  "catalog_file": "https://cmip6-nc.s3.us-east-2.amazonaws.com/esgf-world.csv.gz",
  "attributes": [
    {
      "column_name": "project",
      "vocabulary": ""
    },
    {
      "column_name": "institute",
      "vocabulary": ""
    },
    {
      "column_name": "model",
      "vocabulary": ""
    },
    {
      "column_name": "experiment_id",
      "vocabulary": ""
    },
    { "column_name": "ensemble_member", "vocabulary": "" },
    {
      "column_name": "frequency",
      "vocabulary": ""
    },
    { "column_name": "variable", "vocabulary": "" },
    {
      "column_name": "modeling_realm",
      "vocabulary": ""
    },
    {
      "column_name": "version",
      "vocabulary": ""
    },
    {
      "column_name": "mip_table",
      "vocabulary": ""
    },
      {
      "column_name": "temporal subset",
      "vocabulary": ""
    }
  ],
  "assets": {
    "column_name": "path",
    "format": "netcdf"
  },
  "aggregation_control": {
    "variable_column_name": "variable",
    "groupby_attrs": [
      "project",
      "institute",
      "model",
      "experiment_id",
      "mip_table"
    ],
    "aggregations": [
      {
        "type": "union",
        "attribute_name": "variable"
      },
      {
        "type": "join_existing",
        "attribute_name": "temporal subset",
        "options": { "dim": "time", "coords": "minimal", "compat": "override" }
      },
      {
        "type": "join_new",
        "attribute_name": "ensemble_member",
        "options": { "coords": "minimal", "compat": "override" }
      }
    ]
  }
}

